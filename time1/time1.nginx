upstream time1_app {
	server localhost:5001;
}

server {
	listen 80;
	listen [::]:80;

	server_name time1.mgrathena.info;

	location / {
		proxy_pass http://time1_app;
	}

	location /socket {
		proxy_pass http://time1_app;
		proxy_http_version 1.1;
		proxy_set_header Upgrade $http_upgrade;
		proxy_set_header Connection "upgrade";
	}
}
